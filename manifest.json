{
  "manifest_version": 3,
  "name": "Github print file",
  "version": "1.1",

  "description": "Add print icon to github file preview that strips out ui elements when printing",

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [{
    "matches": ["https://github.com/**"],
    "js": ["index.js"],
    "css": ["styles.css"]
  }]
}
